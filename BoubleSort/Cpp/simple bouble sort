void troca(int *a, int *b)  
{  
    int aux = *a;  
    *a = *b;  
    *b = aux;  
}  
  
void selectionSort(int *vetor, int n)  
{  
    int i, j, comeco;  
  
    for (i = 0; i < n; i++)  
    {  
		for(j = i; j < n; j++){
			if(vetor[j] > vetor[j+1]){
				&troca(vetor[j], &vetor[j+1])
			}
		}
    }  
}  
